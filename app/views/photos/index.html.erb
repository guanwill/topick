<div class="page-header"><h1>Photos</h1></div>

<div class="media">
  <div class="media-left">
    <% @photos.each do |photo| %>
      <%= link_to image_tag(photo.image.url(:thumb), class: 'media-object'), photo.image.url, target: '_blank' %>
    <%end%>

  </div>
  <div class="media-body">
  </div>
</div>
